{
    "version": 2,
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "builds": [
        {
            "src": "server/package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/src/app.js"
        }
    ]
}
